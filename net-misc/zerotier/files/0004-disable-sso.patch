From 2169e128999c0c15dc647bc99e4a624371107995 Mon Sep 17 00:00:00 2001
From: Kay Shinonome <kayshinonome@protonmail.com>
Date: Tue, 12 Apr 2022 04:20:28 -0500
Subject: [PATCH 4/5] disable sso

---
 make-linux.mk      | 2 ++
 node/Constants.hpp | 2 +-
 2 files changed, 3 insertions(+), 1 deletion(-)

diff --git a/make-linux.mk b/make-linux.mk
index 78d82938..9d63df7b 100644
--- a/make-linux.mk
+++ b/make-linux.mk
@@ -252,6 +252,8 @@ ifeq ($(CC_MACH),riscv64)
 	ZT_ARCHITECTURE=0
 endif
 
+ZT_SSO_SUPPORTED=0
+
 # Fail if system architecture could not be determined
 ifeq ($(ZT_ARCHITECTURE),999)
 ERR=$(error FATAL: architecture could not be determined from $(CC) -dumpmachine: $CC_MACH)
diff --git a/node/Constants.hpp b/node/Constants.hpp
index 930c9129..401a2365 100644
--- a/node/Constants.hpp
+++ b/node/Constants.hpp
@@ -52,7 +52,7 @@
 #include <endian.h>
 
 #if (defined(__amd64) || defined(__amd64__) || defined(__x86_64) || defined(__x86_64__) || defined(__AMD64) || defined(__AMD64__) || defined(_M_X64) || defined(__aarch64__))
-#define OIDC_SUPPORTED 1
+#define OIDC_SUPPORTED 0
 #else
 #define OIDC_SUPPORTED 0
 #endif
-- 
